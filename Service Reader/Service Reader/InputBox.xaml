<Window x:Class="Service_Reader.InputBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Service_Reader"
        mc:Ignorable="d"
        Title="Input Box" Height="160" Width="254.842" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="lblInputText" Content="Label" HorizontalAlignment="Left" Margin="0,10,-24,0" VerticalAlignment="Top" Width="218"/>
        <TextBox x:Name="txtInputData" HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="208"/>
        <Button x:Name="cmdOk" Content="Ok" HorizontalAlignment="Left" Margin="143,91,0,0" VerticalAlignment="Top" Width="75" Click="cmdOk_Click"/>
        <Button x:Name="cmdCancel" Content="Cancel" HorizontalAlignment="Left" Margin="32,91,0,0" VerticalAlignment="Top" Width="75" Click="cmdCancel_Click"/>

    </Grid>
</Window>
